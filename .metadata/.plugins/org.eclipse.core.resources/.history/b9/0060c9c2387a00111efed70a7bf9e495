package com.savefish.physics.resolve;

/********************************
 * Description: 该类定义了EdgeShape的
 *              属性，便于反/序列化
 * Author     : 王志伟
 * Date       : 2012/03/12
 *******************************/

public class GreenEdgeShape {
	public GreenVector2 vertex1;
	public GreenVector2 vertex2;
	public boolean hasVertex0;
	public boolean hasVertex3;
	public GreenVector2 vertex0;
	public GreenVector2 vertex3;
}
