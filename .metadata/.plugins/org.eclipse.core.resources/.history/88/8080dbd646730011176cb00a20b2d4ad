package com.savefish.service;

import com.badlogic.gdx.utils.Logger;
import com.savefish.constant.Constants;

public class GreenLogger {
	
	private  Logger logger = null;
	private static GreenLogger gl = new GreenLogger();
	
	private GreenLogger(){
		logger = new Logger(Constants.logger.LOGGER_NAME);
	}
	
	public static GreenLogger getInstance(){
		return gl;
	}

}
